{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ustadmobile": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {
                
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/ustadmobile",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.css",
                            "./node_modules/materialize-css/dist/css/materialize.min.css"
                        ],
                        "scripts": [
                            "./node_modules/jquery/dist/jquery.min.js",
                            "./node_modules/materialize-css/dist/js/materialize.min.js"
                        ],
                        "es5BrowserSupport": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "5mb",
                                    "maximumError": "8mb"
                                }
                            ]
                        },
                        "en": {
                            "i18nFile": "src/assets/locale/messages.en.xlf",
                            "outputPath": "dist/en/",
                            "aot": true,
                            "i18nFormat": "xlf",
                            "i18nLocale": "en",
                            "i18nMissingTranslation": "error"
                        },
                        "ps": {
                            "i18nFile": "src/assets/locale/messages.ps.xlf",
                            "outputPath": "dist/ps/",
                            "aot": true,
                            "i18nFormat": "xlf",
                            "i18nLocale": "ps",
                            "i18nMissingTranslation": "error"
                        },
                        "fa": {
                            "i18nFile": "src/assets/locale/messages.fa.xlf",
                            "outputPath": "dist/fa/",
                            "aot": true,
                            "i18nFormat": "xlf",
                            "i18nLocale": "fa",
                            "i18nMissingTranslation": "error"
                        },
                        "ar": {
                            "i18nFile": "src/assets/locale/messages.ar.xlf",
                            "outputPath": "dist/ar/",
                            "aot": true,
                            "i18nFormat": "xlf",
                            "i18nLocale": "ar",
                            "i18nMissingTranslation": "error"
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "ustadmobile:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "ustadmobile:build:production"
                        },
                        "en": {
                            "browserTarget": "ustadmobile:build:en"
                        },
                        "ps": {
                            "browserTarget": "ustadmobile:build:ps"
                        },
                        "fa": {
                            "browserTarget": "ustadmobile:build:fa"
                        },
                        "ar": {
                            "browserTarget": "ustadmobile:build:ar"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "ustadmobile:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "karmaConfig": "src/karma.conf.js",
                        "styles": [
                            "src/styles.css"
                        ],
                        "scripts": [
                            
                        ],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        },
        "ustadmobile-e2e": {
            "root": "e2e/",
            "projectType": "application",
            "prefix": "",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "e2e/protractor.conf.js",
                        "devServerTarget": "ustadmobile:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "ustadmobile:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "e2e/tsconfig.e2e.json",
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "defaultProject": "ustadmobile"
}
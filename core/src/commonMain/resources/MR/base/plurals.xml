<?xml version="1.0" encoding="UTF-8" ?>
<resources>
    <!--
    plurals are used to handle different languages that require special treatment
    e.g number 0 in arabic
    possible quantities: zero, one, two, few, many, and other.

    Note: this uses plural NOT plural(s) as per Moko resources docs
    -->


    <plural name="number_of_attempts">
        <item quantity="one">%d attempt</item>
        <item quantity="other">%d attempts</item>
    </plural>
    <plural name="duration_hours">
        <item quantity="one">%d hour</item>
        <item quantity="other">%d hours</item>
    </plural>
    <plural name="duration_minutes">
        <item quantity="one">%d min</item>
        <item quantity="other">%d mins</item>
    </plural>
    <plural name="duration_seconds">
        <item quantity="one">%d sec</item>
        <item quantity="other">%d secs</item>
    </plural>
    <plural name="items_selected">
        <item quantity="zero">%d items selected</item>
        <item quantity="one">%d item selected</item>
        <item quantity="two">%d item selected</item>
        <item quantity="few">%d items selected</item>
        <item quantity="many">%d items selected</item>
        <item quantity="other">%d items selected</item>
    </plural>

    <plural name="items_deleted">
        <item quantity="one">%d item deleted</item>
        <item quantity="other">%d items deleted</item>
    </plural>

    <plural name="items_restored">
        <item quantity="one">%d item restored</item>
        <item quantity="other">%d items restored</item>
    </plural>

    <plural name="are_you_sure_you_want_to_permanently_delete">
        <item quantity="one">Are you sure you want to permanently delete %d item?</item>
        <item quantity="other">Are you sure you want to permanently delete %d items?</item>
    </plural>

</resources>
